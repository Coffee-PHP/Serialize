{
    "name": "coffeephp/serialize",
    "description": "Provides an interface for serializer classes to implement.",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Danny Damsky",
            "email": "dannydamsky99@gmail.com"
        }
    ],
    "version": "1.1.2",
    "readme": "README.md",
    "time": "2021-04-23",
    "keywords": [
        "coffeephp",
        "coffee",
        "php",
        "serialize",
        "serialization"
    ],
    "require": {
        "php": ">=8.0",
        "ext-apcu": "*"
    },
    "require-dev": {
        "coffeephp/quality-tools": "1.*"
    },
    "autoload": {
        "psr-4": {
            "CoffeePhp\\Serialize\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "apcu-autoloader": true,
        "platform": {
            "php": "8.0"
        },
        "platform-check": false
    },
    "scripts": {
        "coffeephp:exec:phpunit": "coffeephp_exec_phpunit --testsuite unit,integration",
        "coffeephp:exec:psalm": "coffeephp_exec_psalm",
        "coffeephp:exec:publish": "coffeephp_exec_publish"
    }
}
